{
  "_version": "1.59.0",
  "sap.app": {
    "id": "cap.euro.bettor.soccer",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.5",
      "toolsId": "9cbd84ef-13e5-40b6-a690-fcc7e9c285ee"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/ces/FM_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "match-bet": {
          "semanticObject": "match",
          "action": "bet",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.124.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cap.euro.bettor.soccer.i18n.i18n"
        }
      },
      "mainModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "UpdateGroup": {
              "submit": "API"
            }
          },
          "updateGroupId": "UpdateGroup"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "cap.euro.bettor.soccer.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "flexibleColumnLayout",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "matchList",
          "pattern": "",
          "target": "MatchList"
        },
        {
          "pattern": "betMatch/{matchId}/{layout}",
          "name": "betMatch",
          "target": [
            "MatchList",
            "BetMatch"
          ]
        }
      ],
      "targets": {
        "MatchList": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MatchList",
          "viewName": "MatchList"
        },
        "BetMatch": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "BetMatch",
          "viewName": "BetMatch",
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "cap.euro.bettor.soccer.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "cap.euro.bettor.soccer"
  }
}